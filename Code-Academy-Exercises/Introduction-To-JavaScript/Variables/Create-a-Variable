var favoriteFood = 'pizza';
var numOfSlices = 8;
console.log(favoriteFood)
console.log(numOfSlices)

//Var or variable and myName or the variable's name. = is the assignment operator and 'Arya' is the value assigned. Used var and logged in console

let changeMe = true;

changeMe = false;

console.log(changeMe);

//"Let" keywords signal that the varable can be reassigned a different value

const entree='Enchiladas'
console.log(entree); //Output: Enchiladas
entree="Tacos"

//"Const" keyword also allows you to store any value, but may not be reassigned as you will recieve a "Type Error"
